@model Playground.Mvc.Models.EmployeeMultiSelectListViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Multi Select List Box </h2>

@using (Html.BeginForm("Index", "MultiSelectListBox"))
{
    @Html.ListBox("selectedEmployees", Model.EmployeesList, new { @style = "width:200px;height:150px;" })
    <br /><br />
    <input type="submit" value="Submit" />
    <br />
    <div>
        <br />
        @ViewBag.SelectedEmployees
        <br />
    </div>
}